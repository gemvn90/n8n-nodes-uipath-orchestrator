{
  "name": "n8n-nodes-uipath-orchestrator",
  "version": "1.2.0",
  "description": "n8n node for UiPath Orchestrator API",
  "keywords": [
    "n8n-community-node-package",
    "uipath",
    "orchestrator",
    "rpa"
  ],
  "license": "MIT",
  "author": "Your Name <your.email@example.com>",
  "homepage": "N\\A",
  "repository": {
    "type": "git",
    "url": "N\\A"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "n8n-node dev",
    "build": "n8n-node build",
    "build:watch": "n8n-node build --watch",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "test": "node -r ts-node/register test/unit.test.ts",
    "test:unit": "NODE_ENV=test node -r ts-node/register test/unit.test.ts",
    "test:watch": "nodemon --ext ts --exec 'npm run test' test/",
    "test:assets": "mocha --require ts-node/register test/Assets.test.ts",
    "test:queues": "mocha --require ts-node/register test/Queues.test.ts",
    "test:auditlogs": "mocha --require ts-node/register test/AuditLogs.test.ts",
    "test:buckets": "mocha --require ts-node/register test/Buckets.test.ts",
    "test:directoryservice": "mocha --require ts-node/register test/DirectoryService.test.ts",
    "test:folders": "mocha --require ts-node/register test/Folders.test.ts",
    "test:jobs": "mocha --require ts-node/register test/Jobs.test.ts",
    "test:processes": "mocha --require ts-node/register test/Processes.test.ts",
    "test:robots": "mocha --require ts-node/register test/Robots.test.ts",
    "test:sessions": "mocha --require ts-node/register test/Sessions.test.ts",
    "test:all": "mocha --require ts-node/register test/*.test.ts"
  },
  "n8n": {
    "nodes": [
      "dist/nodes/UiPathOrchestrator/UiPathOrchestrator.node.js"
    ],
    "credentials": [
      "dist/credentials/UiPathOAuth2.credentials.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "latest",
    "@types/chai": "^5.2.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/proxyquire": "^1.3.31",
    "chai": "^6.2.1",
    "jest": "^29.0.0",
    "jest-mock-extended": "^3.0.0",
    "mocha": "^11.7.5",
    "n8n-core": "latest",
    "n8n-workflow": "latest",
    "nodemon": "^3.0.0",
    "proxyquire": "^2.1.3",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.0.0"
  }
}
